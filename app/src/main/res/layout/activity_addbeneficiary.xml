<?xml version="1.0" encoding="utf-8"?>
 <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.temenos.dshubhamrajput.genericnet.AddBeneficiary">
   <CheckBox
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:checked="true"
       android:text="Within bank"
       android:theme="@style/MyCheckBox"
       android:id="@+id/withinbank"
       android:layout_marginStart="46dp"
       android:layout_marginTop="32dp"
       android:layout_alignParentTop="true"
       android:layout_alignParentStart="true" />
   <CheckBox
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:checked="false"
       android:text="NEFT/IMPS"
       android:theme="@style/MyCheckBox"
       android:id="@+id/neft"
       android:layout_marginStart="38dp"
       android:layout_alignBaseline="@+id/withinbank"
       android:layout_alignBottom="@+id/withinbank"
       android:layout_toEndOf="@+id/withinbank" />

   <TextView
       android:id="@+id/textView1"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Beneficiary Acc No"
       android:textColor="@color/BLACK"
       android:layout_marginEnd="15dp"
       android:layout_alignTop="@+id/BeneAccNo"
       android:layout_toStartOf="@+id/BeneAccNo" />

   <EditText
       android:id="@+id/BeneAccNo"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:ems="10"
       android:inputType="number"
       android:background="@drawable/acctedit"
       android:layout_below="@+id/withinbank"
       android:layout_alignParentEnd="true"
       android:layout_marginEnd="16dp" />
   <TextView
       android:id="@+id/textView2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Reenter Acc No"
       android:layout_marginEnd="15dp"
       android:textColor="@color/BLACK"
       android:layout_alignTop="@+id/Renter"
       android:layout_toStartOf="@+id/Renter" />

   <EditText
       android:id="@+id/Renter"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:ems="10"
       android:inputType="number"
       android:background="@drawable/acctedit"
       android:layout_below="@+id/BeneAccNo"
       android:layout_toEndOf="@+id/textView1" />
   <TextView
       android:id="@+id/textView3"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Email"
       android:layout_marginEnd="15dp"
       android:textColor="@color/BLACK"
       android:layout_alignTop="@+id/Email"
       android:layout_toStartOf="@+id/Email" />

   <EditText
       android:id="@+id/Email"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:ems="10"

       android:background="@drawable/acctedit"
       android:layout_below="@+id/Renter"
       android:layout_toEndOf="@+id/textView2" />
   <TextView
       android:id="@+id/textView4"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="Nickname"
       android:layout_marginEnd="15dp"
       android:textColor="@color/BLACK"
       android:layout_alignTop="@+id/Nickname"
       android:layout_toStartOf="@+id/Nickname" />

   <EditText
       android:id="@+id/Nickname"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:ems="10"

       android:background="@drawable/acctedit"
       android:layout_below="@+id/Email"
       android:layout_toEndOf="@+id/textView3" />
   <TextView
       android:id="@+id/textView5"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="IFSC code"
       android:textColor="@color/BLACK"
       android:visibility="invisible"
       android:layout_marginEnd="15dp"
       android:layout_alignTop="@+id/IFSC"
       android:layout_toStartOf="@+id/IFSC" />

   <EditText
       android:id="@+id/IFSC"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_marginTop="24dp"
       android:ems="10"

       android:background="@drawable/acctedit"
       android:layout_below="@+id/Nickname"
       android:layout_toEndOf="@+id/textView4"
       android:visibility="invisible"/>

   <Button
       android:id="@+id/button"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textColor="#ffffff"
       android:background="@drawable/mybutton"
       android:text="Submit"
       android:layout_below="@+id/IFSC"
       android:layout_centerHorizontal="true"
       android:layout_marginTop="85dp"
       android:textSize="20sp"
       />

</RelativeLayout>